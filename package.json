    "@babel/preset-typescript": "^7.16.7",
    "@eslint/create-config": "^0.1.2",
    "@types/react-dom": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
